<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="150dp"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <stroke
                android:width="8dp"
                android:color="@color/markBlue"
                />


        </shape>
    </item>
    <item android:drawable="@drawable/btn_compass_round" />
</layer-list>